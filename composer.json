{
    "name": "fw/last-bundle",
    "type": "symfony-bundle",
    "description": "Static site generator for any Symfony 4 project.",
    "require": {
        "symfony/framework-bundle": "^4.1|^5.0",
        "symfony/yaml": "^4.1|^5.0",
        "symfony/console": "^4.1|^5.0"
    },
    "autoload": {
        "psr-4": { "Fw\\LastBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": { "Tests\\App\\": "Tests/app/src" }
    },
    "authors": [
        {
            "name": "Franz Wilding",
            "email": "franz@unite.co.at"
        }
    ],
    "require-dev": {
        "symfony/phpunit-bridge": "^4.1|^5.0",
        "symfony/twig-bundle": "^4.1|^5.0",
        "symfony/asset": "^4.1|^5.0"
    }
}
